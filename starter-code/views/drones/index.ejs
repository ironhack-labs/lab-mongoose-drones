<h2> Drones </h2>

<a href="/drones/new"> Add a New Drone </a>

<ul>
  <% listOfDrones.forEach( (oneDrone) => { %>
    <li>
      <h3> Drone: <%= oneDrone.name %> </h3>
      <p> Number of Propellers: <%= oneDrone.propellers %> </p>
      <p> Maximum Speed (meters per second): <%= oneDrone.maxSpeed %> </p>
    </li>
  <% }) %>
</ul>
