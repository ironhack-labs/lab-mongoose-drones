<h2>Drones</h2>

<ul>
    <% drones.forEach((drone) => { %>
      <!-- display info from aDrone -->
      <li>
        <%= drone.name %> <%= drone.propellers %> <%= drone.maxSpeed %>
      </li>
    <% }) %>
  </ul>

  <button class="btn btn-lg btn-primary"><a href="/drones/new">Add A Drone</a></button>
  <br> <br> 
  <button class="btn btn-lg btn-primary"><a href="/">Go Back Home</a></button>