<h2>Drones</h2>
<a href="/drones/new">New drones</a>
<ul>
<% drones.forEach( (drone) => { %>
    <li><%= drone.name %></li>
    <li><%= drone.propellers %></li>
    <li><%= drone.maxSpeed %></li>
    <a href="/drones/<%= drone._id %>/edit">Edit</a>
    <form action="/drones/<%= drone._id %>/delete" method="post">
        <input type="submit" value="Delete">
    </form>
<% }); %>
</ul>

