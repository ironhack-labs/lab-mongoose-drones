<h2>List of drones</h2>
<h5 class="bg-info">Add a new drone clicking <a href="/drones/new"> here </a></h5>
<ul >
  <% drones.forEach((aDrone) => { %>
    <li> Drone name: <%= aDrone.name %> </li>
    <li class="list-unstyled"> Nr of propellers: <%= aDrone.propellers %> </li>
    <li class="list-unstyled"> Max speed: <%= aDrone.maxSpeed %> </li> <br>
  <% }) %>
</ul>
