<ul class="list-group col-sm-4">
  <% drones.forEach((drone) => { %>
    <li class="list-group-item">
      <h4><%= drone.name %></h4> 
	  <p>Propellers: <%= drone.propellers %>, Max speed: <%= drone.maxSpeed %></p>
    </li>
  <%  }) %>
</ul>
<div class="col-sm-4">
	<a href="/drones/new" class="btn btn-success">Create new</a>
</div>